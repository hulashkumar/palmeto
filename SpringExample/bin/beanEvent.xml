<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="helloworld" class="eventHandling.HelloWorld">
	<property name="message" value="HelloWorld!"/>
	</bean>
	<bean id="cStartEventHandler" class="eventHandling.cStartEventHandler"></bean>
	<bean id="cStopEventHandler" class="eventHandling.cStopEventHandler"></bean>
	<bean id="cRefreshedEventHandler" class="eventHandling.cRefreshedEventHandler"></bean>

	</beans>