import java.util.Scanner;
public class UniqueChar{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
       int l=s.length();
       char a[]=new char[l];
       for(int i=0;i>l;i++) {
    	   a[i]=s.charAt(i);
       }
        System.out.println(l);
    }
}