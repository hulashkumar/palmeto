import java.util.Scanner;
public class Test{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter total no.of employees:");
        int n=sc.nextInt();
        System.out.println("Enter the age for "+n+" employees:");
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=sc.nextInt();
        }
        
    }
}