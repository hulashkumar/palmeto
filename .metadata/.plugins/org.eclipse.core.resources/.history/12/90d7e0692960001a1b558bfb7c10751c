import java.util.Scanner;
public class BikeRace{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        String n=sc.next();
        
        int p1=1,p2=1;
        for(int i=1;i<=n.length();i++){
        	if(i%2==0) {
        		p1=p1*n.charAt(i);
        	}else {
        		p2=p2*n.charAt(i);
        	}
        }
        int b;
        if(p1>p2) {
        	b=p1;
        }else {
        	b=p2;
        }
          System.out.println(b);  
        
    }
}