import java.util.Scanner;
public class OccurrenceOfChar{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        String s=sc.next();
        int l=s.length();
        String a[]=new String[l];
        for(int i=0;i<l;i++){
            a[i]=s.charAt(i);
        }
    }
}