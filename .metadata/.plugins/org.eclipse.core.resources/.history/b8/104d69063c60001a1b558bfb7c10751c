import java.util.Scanner;
public class OccurrenceOfChar{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        String s=sc.next();
        int l=s.length();
        char a[]=new char[l];
        for(int i=0;i<l;i++){
            a[i]=s.charAt(i);
        }
        int count=0;
        for(int i=0;i<l;i++){
        	for(int j=1;j<i;j++) {
        		
        		if(a[i]==a[j]) {
        			count++;
        		}
        	}
        }
        System.out.println(count);
        
    }
}